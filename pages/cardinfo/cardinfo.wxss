 @import '../../style/weui.wxss'; 

body { background-color: #fff; }
.brief-info { text-align: center; padding: 12px; position: relative; }
.brief-info .err { display: block; position: absolute; right: 6px; top: 6px; width: 32px; height: 32px; background: url(data:image/png;base64,) no-repeat center; background-size: 20px 20px; }
.brief-info wx-image { width: 200px; height: 128px; }
.card-name { font-size: 14px; }
.des, .basic-info { font-size: 10px; color: #9b9a9b; }
.brief-info .des { display: -webkit-box; -webkit-box-orient: vertical; -webkit-line-clamp: 2; overflow: hidden; }
.basic-info wx-text { margin: 0 4px; }
.basic-info .dash { margin: 0; }
.space { width: 100%; height: 10px; background-color: #f8f8f8; }
.tab-content { padding: 12px; }
.tag-name { font-size: 14px; height: 20px; vertical-align: middle; }
.tag-name wx-image { width: 16px; height: 16px; vertical-align: middle; margin-right: 6px; }
.rights-ul { padding: 6px 0 6px 30px; color: #9b9a9b; font-size: 14px; }
.rights-li { display: flex; justify-content: space-between; position: relative; padding: 2px 0; }
.rights-li::before { content: \x27•\x27; position: absolute; left: -10px; }
.points-item { padding-bottom: 12px; }
.points-item>wx-text { font-size: 14px; color: #000; }
.points-ul { padding: 6px 0 6px 30px; font-size: 14px; color: #9b9a9b; }
.points-li { position: relative; padding: 2px 0; }
.points-li::before { content: '•'; position: absolute; left: -10px; }
.online-pay wx-text { width: 49%; display: inline-block; font-size: 14px; color: #9b9a9b; }
.rich-text { font-size: 14px; color: #9b9a9b; }
.rich-text wx-view { margin-bottom: 12px; }
.tip-item { border-bottom: ",1," solid #ccc; font-size: 14px; padding-bottom: 12px; margin-bottom: 12px; }
.tip-item wx-text { color: #9b9a9b; display: -webkit-box; -webkit-box-orient: vertical; -webkit-line-clamp: 2; overflow: hidden; }
.no-data { padding-top: 60px; text-align: center; font-size: 14px; color: #b8b8b8; }
.btn-group { text-align: center; position: absolute; width: 100%; left: 0; padding: 50px 0; display: flex; }
.btn-group>wx-view { flex: 1; width: 50%; font-size: 16px; border: ",1," solid #e4e4e4; padding: 10px 0; position: relative; }
.btn-group>wx-view.active { background-color: #e4e4e4; }
.btn-group .tip { position: absolute; width: 100%; bottom: 0; left: 0; font-size: 10px; color: #9b9b9b; }
.btn2 { flex: 1; width: 50%; font-size: 16px; border: ",1," solid #e4e4e4; padding: 10px 0; position: relative; box-sizing: border-box; }
.btn2 wx-text { color: #479541; }
