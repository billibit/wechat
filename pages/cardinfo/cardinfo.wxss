.has wx-text { color: #9b9b9b; }
.has wx-text::before { content: ''; display: inline-block; width: 16px; height: 16px; background: url(data:image/png;base64,\x3d) no-repeat; background-size: 100%; margin-right: 5px; position: relative; bottom: -1px; }