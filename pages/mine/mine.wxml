<page>
  <view class="page">
    <view class="head">
      <view class="blur"></view>
      <view class="headimg">
        <image class="img" src="{{datas.headImg}}"></image>
        <view>{{datas.nickname}}</view>
      </view>
    </view>
    <view class="card-info">
      <navigator hoverClass="none" class="item" openType="switchTab" url="/pages/cardpack/cardpack">
        <text>信用卡</text>
        <view class="num">{{datas.cardNum}}</view>
        <view class="des">额度：￥{{datas.quotaNum}}</view>
      </navigator>
      <navigator hoverClass="none" openType="switchTab" class="item mid" url="/pages/bill/bill">
        <text>账单</text>
        <view class="num">{{datas.billNum}}</view>
        <view class="des">待还：￥{{datas.billAmount}}</view>
      </navigator>
      <view class="item">
        <text style="visibility:hidden">0</text>
        <view class="num">敬请期待</view>
        <view class="des" style="visibility:hidden">0</view>
      </view>
    </view>
    <view class="weui-cells weui-cells_after-title">
      <navigator hoverClass="weui-cell_active" class="weui-cell weui-cell_access" url="/pages/managetags/managetags">
        <view class="weui-cell__hd">
          <image src="{{card}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
        </view>
        <view class="weui-cell__bd">卡包管理</view>
        <view class="weui-cell__ft weui-cell__ft_in-access"></view>
      </navigator>
      <navigator hoverClass="weui-cell_active" class="weui-cell weui-cell_access" url="/pages/annulfee/annulfee">
        <view class="weui-cell__hd">
          <image style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;" src="{{fee}}"></image>
        </view>
        <view class="weui-cell__bd">年费管理</view>
        <view class="weui-cell__ft weui-cell__ft_in-access"></view>
      </navigator>
      <navigator hoverClass="weui-cell_active" class="weui-cell weui-cell_access" url="/pages/help/help">
        <view class="weui-cell__hd">
          <image style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;" src="{{help}}"></image>
        </view>
        <view class="weui-cell__bd">帮助</view>
        <view class="weui-cell__ft weui-cell__ft_in-access"></view>
      </navigator>
      <view hoverClass="weui-cell_active" class="weui-cell weui-cell_access">
        <view class="weui-cell__hd">
          <image style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;" src="{{msg}}"></image>
        </view>
        <button hoverClass="none" class="weui-cell__bd contact" openType="contact">客服</button>
        <view class="weui-cell__ft weui-cell__ft_in-access"></view>
      </view>
    </view>
    <view class="weui-cells weui-cells_after-title" style="margin-top: 10px">
      <navigator hoverClass="weui-cell_active" class="weui-cell weui-cell_access" url="/pages/helpinfo/helpinfo?id=11">
        <view class="weui-cell__hd">
          <image style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;" src="{{about}}"></image>
        </view>
        <view class="weui-cell__bd">关于</view>
        <view class="weui-cell__ft weui-cell__ft_in-access"></view>
      </navigator>
    </view>
    <view class="tip">
      <view>enjoy life</view>
    </view>
  </view>
</page>