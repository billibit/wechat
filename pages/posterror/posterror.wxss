@import '../../style/weui.wxss';
body { background-color: #fff; }
.page { padding: 0 12px; }
.title { font-size: 18px; color: #1aad19; text-align: center; padding: 20px 0; }
wx-text { font-size: 14px; }
.act-title { font-size: 12px; color: #9b9b9b; padding-bottom: 10px; }
.radio { display: block; margin-top: 6px; font-size: 12px; color: #9b9b9b; }
.radio wx-radio { margin-right: 5px; }
.radio-group { padding-bottom: 10px; }
wx-textarea { width: 100%; min-height: 50px; padding: ",10," 0; border: 1px solid #d9d9d9; border-radius: 5px; text-align: justify; font-size: 12px; color: #9b9b9b; }
wx-button { margin-top: 20px; width: 30%; }